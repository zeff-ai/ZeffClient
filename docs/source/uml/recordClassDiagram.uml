@startuml

namespace ZeffClient.record {
	class Record {
		name: String
		void validate()
		void submit()
	}
	Record "1" *-- "0..*" StructuredData
	Record "1" *-- "0..*" UnstructuredData

	class StructuredData {
		name: String
		value: Bytes
		data_type: [continuous, discrete]
		target: [yes, no, ignore]
	}

	class UnstructuredData{
		data: URI
		media_type: RFC2046
		group_by: String
	}
}

@enduml
